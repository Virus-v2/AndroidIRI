<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".MainActivity">

    <TextView
            android:text="X"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/phone_acc_x"
            app:layout_constraintStart_toStartOf="@+id/textView6"
            app:layout_constraintEnd_toEndOf="@+id/textView6"
            android:layout_marginTop="8dp" app:layout_constraintTop_toBottomOf="@+id/textView6"/>
    <TextView
            android:text="Y"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/phone_acc_y" android:layout_marginTop="8dp"
            app:layout_constraintTop_toBottomOf="@+id/phone_acc_x"
            app:layout_constraintStart_toStartOf="@+id/phone_acc_x"
            app:layout_constraintEnd_toEndOf="@+id/phone_acc_x"/>
    <TextView
            android:text="Z"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/phone_acc_z" android:layout_marginTop="8dp"
            app:layout_constraintTop_toBottomOf="@+id/phone_acc_y"
            app:layout_constraintStart_toStartOf="@+id/phone_acc_y"
            app:layout_constraintEnd_toEndOf="@+id/phone_acc_y"/>
    <View
            android:id="@+id/divider"
            android:layout_width="368dp"
            android:layout_height="1dp"
            android:background="?android:attr/listDivider"
            tools:layout_editor_absoluteX="8dp" android:layout_marginTop="8dp"
            app:layout_constraintTop_toTopOf="parent"
            android:layout_marginBottom="8dp" app:layout_constraintBottom_toTopOf="@+id/divider2"/>
    <TextView
            android:text="Z"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/earth_acc_z" android:layout_marginTop="8dp"
            app:layout_constraintTop_toBottomOf="@+id/earth_acc_y"
            app:layout_constraintStart_toStartOf="@+id/earth_acc_y"
            app:layout_constraintEnd_toEndOf="@+id/earth_acc_y"
    />
    <TextView
            android:text="Y"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/earth_acc_y" android:layout_marginTop="8dp"
            app:layout_constraintTop_toBottomOf="@+id/earth_acc_x"
            app:layout_constraintStart_toStartOf="@+id/earth_acc_x"
            app:layout_constraintEnd_toEndOf="@+id/earth_acc_x"
    />
    <TextView
            android:text="X"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/earth_acc_x" android:layout_marginTop="8dp"
            app:layout_constraintTop_toBottomOf="@+id/textView2" app:layout_constraintStart_toStartOf="@+id/textView2"
            app:layout_constraintEnd_toEndOf="@+id/textView2"/>
    <TextView
            android:text="EARTH"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/textView2" android:layout_marginTop="32dp"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="8dp" app:layout_constraintEnd_toEndOf="parent" android:layout_marginEnd="8dp"/>
    <TextView
            android:text="PHONE"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/textView6"
            app:layout_constraintTop_toBottomOf="@+id/divider"
            app:layout_constraintStart_toStartOf="parent" android:layout_marginStart="8dp"
            app:layout_constraintEnd_toEndOf="parent" android:layout_marginEnd="8dp" android:layout_marginTop="32dp"/>
    <TextView
            android:text="X: "
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/textView7" app:layout_constraintEnd_toStartOf="@+id/earth_acc_x"
            android:layout_marginEnd="8dp" app:layout_constraintTop_toTopOf="@+id/earth_acc_x"
    />
    <TextView
            android:text="Y: "
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/textView8" app:layout_constraintEnd_toStartOf="@+id/earth_acc_y"
            android:layout_marginEnd="8dp" app:layout_constraintTop_toTopOf="@+id/earth_acc_y"
    />
    <TextView
            android:text="Z: "
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/textView9" app:layout_constraintEnd_toStartOf="@+id/earth_acc_z"
            android:layout_marginEnd="8dp" app:layout_constraintTop_toTopOf="@+id/earth_acc_z"
    />
    <TextView
            android:text="X: "
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/textView10" app:layout_constraintEnd_toStartOf="@+id/phone_acc_x"
            android:layout_marginEnd="8dp" app:layout_constraintTop_toTopOf="@+id/phone_acc_x"
    />
    <TextView
            android:text="Y: "
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/textView11" app:layout_constraintEnd_toStartOf="@+id/phone_acc_y"
            android:layout_marginEnd="8dp" app:layout_constraintTop_toTopOf="@+id/phone_acc_y"
    />
    <TextView
            android:text="Z: "
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/textView12" app:layout_constraintEnd_toStartOf="@+id/phone_acc_z"
            android:layout_marginEnd="8dp" app:layout_constraintTop_toTopOf="@+id/phone_acc_z"
    />
    <View
            android:id="@+id/divider2"
            android:layout_width="368dp"
            android:layout_height="1dp"
            android:background="?android:attr/listDivider"
            tools:layout_editor_absoluteX="8dp" android:layout_marginBottom="8dp"
            app:layout_constraintBottom_toBottomOf="parent" app:layout_constraintTop_toTopOf="parent"
            android:layout_marginTop="8dp" app:layout_constraintVertical_bias="0.68"/>
    <TextView
            android:text="ORIENTATION"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/textView"
            app:layout_constraintStart_toStartOf="parent" android:layout_marginStart="8dp"
            app:layout_constraintEnd_toEndOf="parent" android:layout_marginEnd="8dp"
            app:layout_constraintHorizontal_bias="0.501" android:layout_marginTop="32dp"
            app:layout_constraintTop_toBottomOf="@+id/divider2"/>
    <TextView
            android:text="Pitch:"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/textView3" app:layout_constraintEnd_toStartOf="@+id/phone_pitch"
            android:layout_marginEnd="8dp" app:layout_constraintTop_toTopOf="@+id/phone_pitch"
    />
    <TextView
            android:text="Roll:"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/textView4" app:layout_constraintEnd_toStartOf="@+id/phone_roll"
            android:layout_marginEnd="8dp" app:layout_constraintTop_toTopOf="@+id/phone_roll"
    />
    <TextView
            android:text="Azimuth:"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/textView5" app:layout_constraintEnd_toStartOf="@+id/phone_azimuth"
            android:layout_marginEnd="8dp" app:layout_constraintTop_toTopOf="@+id/phone_azimuth"
    />
    <TextView
            android:text="TextView"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/phone_azimuth" android:layout_marginTop="8dp"
            app:layout_constraintTop_toBottomOf="@+id/textView" app:layout_constraintEnd_toEndOf="@+id/textView"
            android:layout_marginEnd="8dp" app:layout_constraintStart_toStartOf="@+id/textView"
            android:layout_marginStart="8dp"/>
    <TextView
            android:text="TextView"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/phone_pitch" android:layout_marginTop="8dp"
            app:layout_constraintTop_toBottomOf="@+id/phone_azimuth" app:layout_constraintEnd_toEndOf="@+id/phone_azimuth"
            app:layout_constraintStart_toStartOf="@+id/phone_azimuth"/>
    <TextView
            android:text="TextView"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/phone_roll" app:layout_constraintStart_toStartOf="@+id/phone_pitch"
            app:layout_constraintEnd_toEndOf="@+id/phone_pitch"
            android:layout_marginTop="8dp" app:layout_constraintTop_toBottomOf="@+id/phone_pitch"/>
    <Button
            android:text="Button"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/startRecording" app:layout_constraintEnd_toEndOf="parent" android:layout_marginEnd="16dp"
            app:layout_constraintBottom_toBottomOf="parent" android:layout_marginBottom="16dp"/>
</android.support.constraint.ConstraintLayout>